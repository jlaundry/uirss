{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "name": "outfile",
      "type": "blob",
      "direction": "out",

      "connection": "PublicWebStorage",
      "path": "web/rss/ui-updates.xml"
    }
  ]
}
